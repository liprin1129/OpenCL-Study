#include <iostream>

void matMul(int *C[], int *A, int*B, int heightA, int widthA, int widthB){
    for (int i = 0; i < heightA; i++) {
        for (int j = 0; j<widthB; j++) {
            C[i][j] = 0;
            for (int k = 0; k<widthA; k++) {
                C[i][j] += A[i][k]*B[k][j];
                std::cout << C[i][j];
            }
        }
    }
}

int main(int argc, const char * argv[]) {
    // insert code here...
    std::cout << "Hello, World!\n";
    return 0;
}

